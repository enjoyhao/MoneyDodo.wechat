/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.flex-row {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.flex-column {
  display: flex;
  flex-direction: column;
  align-items: center;
}

@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1560587967340'); /* IE9 */
  src: url('iconfont.eot?t=1560587967340#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('iconfont.woff?t=1560587967340') format('woff'),
  url('iconfont.ttf?t=1560587967340') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('iconfont.svg?t=1560587967340#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 48px;
  color: yellow;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconfont-jia {
  font-family: "iconfont" !important;
  font-size: 24px;
  color: blue;
  font-style: normal;
  margin: 0rpx 0rpx 0rpx 30rpx;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-qian:before {
  content: "\e631";
}

.icon-jia:before {
  content: "\e65e";
}



